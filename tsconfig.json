    "compilerOptions": {
        "target": "es6",
        "lib": ["esnext"],
        "module": "commonjs",
        "allowSyntheticDefaultImports": true,
        "strictNullChecks": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
    },
    "include": [
        "**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}
    